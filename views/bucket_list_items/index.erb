<div id="bucket_list"
    <% @bucket_list_items.each do |bucket_list| %>
    <% current_city = bucket_list.city %>
    <h2>Bucket List</h2>
      <li><%= current_city.name %> <%= current_city.country.name %> <%= bucket_list.rating %></li>
      <form action="/bucket_list/<%= bucket_list.id %>/delete" method="POST">
      <input type="submit" value="Remove From Bucket List"></form>
</div>


<% end %>
</div>
